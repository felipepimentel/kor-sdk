{
    "on_boot": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/collector.py init",
                "timeout": 5
            }
        }
    ],
    "on_agent_start": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/collector.py record agent_lifecycle agent_start '{\"agent_id\": \"{agent_id}\"}'",
                "timeout": 5
            }
        }
    ],
    "on_agent_end": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/collector.py record agent_lifecycle agent_end '{\"agent_id\": \"{agent_id}\"}'",
                "timeout": 5
            }
        }
    ],
    "on_tool_call": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/collector.py record tool_call tool_invoke '{\"tool_name\": \"{tool_name}\"}'",
                "timeout": 5
            }
        }
    ]
}