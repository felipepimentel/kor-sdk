{
    "on_llm_request": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/tracker.py track '{\"model\": \"{model}\", \"provider\": \"{provider}\", \"prompt_tokens\": {prompt_tokens}, \"completion_tokens\": {completion_tokens}, \"total_tokens\": {total_tokens}}'",
                "timeout": 5
            }
        }
    ],
    "on_boot": [
        {
            "command": {
                "cmd": "python3 ${KOR_PLUGIN_ROOT}/scripts/tracker.py init",
                "timeout": 5
            }
        }
    ]
}